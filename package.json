{
  "name": "ember-mdi",
  "version": "0.6.0",
  "description": "An ember-cli addon for using Material Design Icons in Ember applications.",
  "keywords": [
    "material design icons",
    "mdi",
    "icons",
    "svg",
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kaermorchen/ember-mdi.git"
  },
  "license": "MIT",
  "author": {
    "name": "Stanislav Romanov",
    "email": "kaermorchen@gmail.com",
    "url": "https://stanislavromanov.ru"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@mdi/svg": "^3.0.0",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-sass": "^10.0.0",
    "resolve": "^1.11.1",
    "sass": "^1.19.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "bootstrap": "^4.3.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.9.0",
    "ember-cli-bootstrap-4": "^0.9.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.5.1",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.9.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "^6.4.1",
    "eslint-plugin-node": "^8.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "ember-mdi",
    "demoURL": "https://kaermorchen.github.io/ember-mdi/"
  }
}
